{
    "name": "aplus/config",
    "type": "library",
    "description": "Aplus Framework Config Library",
    "keywords": [
        "config",
        "env",
        "ini",
        "yaml",
        "database"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/libraries/config/",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "aplus/database": "^2.8",
        "aplus/helpers": "^2.2"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Config\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/config/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/config/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/config",
        "docs": "https://docs.aplus-framework.com/guides/libraries/config/"
    },
    "funding": [
        {
            "type": "other",
            "url": "https://aplus-framework.com/sponsor"
        }
    ]
}
