{
    "name": "aplus/config",
    "type": "library",
    "description": "Aplus Framework Config Library",
    "keywords": [
        "config",
        "env",
        "ini",
        "yaml",
        "database"
    ],
    "homepage": "https://aplus-framework.com/docs/config",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-yaml": "*",
        "natanfelles/database": "^2.5",
        "natanfelles/helpers": "^2.1"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "natanfelles/coding-standard": "^1.7",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Config\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/config/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/config/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/config",
        "docs": "https://aplus-framework.gitlab.io/libraries/config/docs/"
    }
}
